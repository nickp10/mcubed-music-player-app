<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="mCubedTheme" parent="@android:style/Theme.Holo">
		<item name="action_about">@drawable/action_about_dark</item>
		<item name="action_exit">@drawable/action_exit_dark</item>
		<item name="action_feedback">@drawable/action_feedback_dark</item>
		<item name="action_help">@drawable/action_help_dark</item>
		<item name="action_library">@drawable/action_library_dark</item>
		<item name="action_nowplaying">@drawable/action_nowplaying_dark</item>
		<item name="action_playall">@drawable/action_playall_dark</item>
		<item name="action_search">@drawable/action_search_dark</item>
		<item name="action_settings">@drawable/action_settings_dark</item>
