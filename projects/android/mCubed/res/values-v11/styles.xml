		<item name="action_nowplaying">@drawable/action_nowplaying_light</item>
		<item name="action_playall">@drawable/action_playall_light</item>
		<item name="action_search">@drawable/action_search_light</item>
		<item name="action_settings">@drawable/action_settings_light</item>
		<item name="border_color">?android:attr/colorActivatedHighlight</item>
		<item name="style_progress_bar">@android:style/Widget.Holo.Light.ProgressBar.Horizontal</item>
	</style>
	<style name="MediaFileGroupDivider" parent="@style/Divider">
		<item name="android:layout_height">2dp</item>
	</style>
</resources>